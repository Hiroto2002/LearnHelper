{
  "name": "learn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "build": "pathpida && next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --dir src --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}",
    "vercel-build": "npx prisma generate && npx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.5.6",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.12.1",
    "@types/node": "18.15.10",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.5",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "supabase": "^1.45.3",
    "swr": "^2.1.2",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@prisma/client": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "prettier": "^2.8.7",
    "prisma": "^4.13.0"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  }
}
